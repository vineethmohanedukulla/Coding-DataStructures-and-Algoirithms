// time complexity = O(n) and space is O(1)

// given here only one element is repeating .
import java.util.*;

public class Main{
    public static void main(String[] args){

        int[] arr = {5,6,7,8,9,5,9,8,7}; // ans = 6
        int ans = 0;
        for(int i = 0; i < arr.length; i++){
            ans = ans ^ arr[i];
        }

        System.out.println(ans);
    }
}